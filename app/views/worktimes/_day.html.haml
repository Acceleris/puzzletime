- daily_worktimes = daily_worktimes(@worktimes, day)
- if daily_worktimes.empty?
  .date-label.empty{'data-date' => day}
    .inner
      = format_day(day)
- else
  .date-label{'data-date' => day}
    .inner
      = format_day(day)
  = render partial: 'worktime', collection: daily_worktimes, as: :worktime, locals: {day: day}
